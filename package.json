{
  "name": "rox_social",
  "version": "1.0.0",
  "description": "Social network learning project",
  "main": "index.js",
  "scripts": {
    "start": "npm run development",
    "start-server": "node $NODE_DEBUG_OPTION ./index.js",
    "development": "npm run build:public:dev && npm run start-server",
    "production": "npm run build:public:prod && npm run start-server",
    "build:public:dev": "webpack --config ./config/webpack/webpack.dev.js",
    "build:public:prod": "webpack --config ./config/webpack/webpack.prod.js",
    "deploy": "npm run deploy:prepare && npm run deploy:execute",
    "deploy:prepare": "npm run build:public:prod && git add . && git commit -m 'Automatic commit for deploing to heroku'",
    "deploy:execute": "git status && git push",
    "test": "jest --coverage --verbose=true",
    "test:sudo": "sudo npm run test"
  },
